<h1>Monitored Item - Edit</h1>
<h5><a href="#" data-wh-icon-size="medium" data-wowhead="item=<%= monitoredItem.itemId %>"></a></h5>

<div class="row">
    <form method="POST" action="/monitoring/edit/<%= monitoredItem.id %>?_method=PUT" class="col s12">
        <div class="row">
            <div class="col s12">
                <div class="row">
                    <div class="input-field col s12">
                        <i class="material-icons prefix">edit</i>
                        <input type="hidden" name="id" value="<%= monitoredItem.id %>">
                        <input type="text" name="realm" id="autocomplete-input" class="autocomplete">
                        <label for="autocomplete-input">New Realm</label>
                        <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                            <i class="material-icons right">send</i>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>

<script>
    document.addEventListener('DOMContentLoaded', function() {
        var autoData = <%- JSON.stringify(autoData) %>
        $(document).ready(function () {
            $('input.autocomplete').autocomplete({
                data: autoData
            });
        });
    })
</script>