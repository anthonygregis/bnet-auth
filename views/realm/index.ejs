<script>const whTooltips = {colorLinks: false, iconizeLinks: false, iconSize: false, renameLinks: true};</script>
<script src="https://wow.zamimg.com/widgets/power.js"></script>
<div class="row">
    <h1>Realm: <%= realmInfo.name %></h1>
</div>
<div class="row">
    <div class="col s12">
        <div class="collection grey darken-4">
            <h4 class="collection-header">Most Popular Items</h4>
            <% mostAvailableItems.forEach(listing => { %>
                <a href="/realms/<%= realmInfo.slug %>/<%= listing.itemId %>" class="collection-item" data-wowhead="item=<%= listing.itemId %>"></a>
            <% }) %>
        </div>
    </div>
</div>
<div class="row">
    <div class="col s12">
        <div class="collection grey darken-4">
            <h4 class="collection-header">Items</h4>
            <% items.forEach(item => { %>
                <a href="/realms/<%= realmInfo.slug %>/<%= item.id %>" class="collection-item" data-wowhead="item=<%= item.id %>" iconizeLinks="true"></a>
            <% }) %>
        </div>
    </div>
</div>