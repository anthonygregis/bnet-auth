<% if (alerts.error) { %>
    <% alerts.error.forEach(msg => { %>
        <script>
            M.toast({html: '<%= msg %>'})
        </script>
    <% })%>
<% } %>

<% if (alerts.success) { %>
    <% alerts.success.forEach(msg => { %>
        <script>
            M.toast({html: '<%= msg %>'})
        </script>
    <% })%>
<% } %>

<% if (alerts.info) { %>
    <% alerts.info.forEach(msg => { %>
        <script>
            M.toast({html: '<%= msg %>'})
        </script>
    <% })%>
<% } %>