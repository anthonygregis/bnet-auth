<ul id="slide-out" class="sidenav sidenav-fixed">
    <li><div class="user-view">
            <div class="background">
                <img src="images/office.jpg">
            </div>
            <% if (currentUser) { %>
                <a href="#user"><img class="circle" src="images/yuna.jpg"></a>
                <a href="#name"><span class="black-text name"><%=  currentUser.id %></span></a>
                <a href="#email"><span class="black-text email"><%=  currentUser.battletag %></span></a>
            <% } %>
        </div></li>
    <li><a href="#!"><i class="material-icons">cloud</i>First Link With Icon</a></li>
    <li><a href="#!">Second Link</a></li>
    <li><div class="divider"></div></li>
    <li><a class="subheader">Your Realms</a></li>
    <% currentUser.characters.forEach(character => { %>
        <li><a class="waves-effect" href="/realm/<%= character.realm.slug %>"><%= character.realm.name %></a></li>
    <% }) %>

</ul>