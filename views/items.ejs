<div class="row">
    <h1>Global Items</h1>
</div>
<div class="row">
    <div class="col s6">
        <div class="collection grey darken-4">
            <h4 class="collection-header">Most Popular Items</h4>
            <% mostAvailableItems.forEach(listing => { %>
                <a href="/realms/area-52/<%= listing.itemId %>" class="collection-item" data-wowhead="item=<%= listing.itemId %>"></a>
            <% }) %>
        </div>
    </div>
</div>